<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/results_container_all"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMain"
    tools:context="com.example.vestibular.vestibulapp.presentation.results.ResultsActivity"
    tools:layout_editor_absoluteY="81dp">

    <ImageView
        android:id="@+id/logo_vestibulapp"
        android:layout_width="215dp"
        android:layout_height="63dp"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/vestibulap_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/linearLayout_button_inicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="16dp"
        card_view:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageButton_inicio"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/button_inicio"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="16dp" />

    </LinearLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/view_pager_results"
        android:layout_width="match_parent"
        android:layout_height="325dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout_button_inicio"
        app:layout_constraintTop_toBottomOf="@+id/logo_vestibulapp"></android.support.v4.view.ViewPager>




</android.support.constraint.ConstraintLayout>
